  "dependencies": {
    "@cavrnus/lib": "^0.3.25"
  },
  "devDependencies": {
    "browserify": "^17.0.0",
    "npx": "^10.2.2",
    "webpack": "^5.76.2",
    "webpack-cli": "^5.0.1"
  }
}
