{
  "name": "webpack-cavrnus-lib",
  "version": "1.0.0",
  "description": "research in creating a cavrnus dist",
  "main": "index.js",
  "scripts": {
    "build": "npm i && npm run build-fast",
    "build-fast": "npx webpack && npm run browerify",
